      <item><label>e</label>478.70 feet; 1207.73 feet</item>
      <item><label>g</label>Yes</item>
      <item><label>h</label>Yes; 
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mfenced open="(" close=")">
                  <m:mrow>
                    <m:mtext>57</m:mtext>
